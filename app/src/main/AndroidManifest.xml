<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:dataExtractionRules="@xml/data_extraction_rules"
            android:fullBackupContent="@xml/backup_rules"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/Theme.MyFIATs"
            tools:targetApi="31">
        <activity
                android:name=".CameraLayoutActivity"
                android:exported="false"/>
        <activity
                android:name=".CurrencyLayoutActivity"
                android:exported="false"/>
        <activity
                android:name=".LoggedLayoutActivity"
                android:exported="false"/>
        <activity
                android:name=".RegisterLayoutActivity"
                android:exported="false"/>
        <activity
                android:name=".LoginLayoutActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>